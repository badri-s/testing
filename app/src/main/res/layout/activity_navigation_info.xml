<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/kentiaraparlamenti"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".NavigationInfoActivity">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/navi"/>
    <ImageView
        android:id="@+id/profile3"
        android:src="@drawable/profile"
        android:layout_width="70dp"
        android:layout_height="70dp"/>
    <ImageView
        android:layout_marginTop="-40dp"
        android:id="@+id/parlamenta"
        android:layout_width="300dp"
        android:layout_height="250dp"
        android:layout_gravity="center"
        android:background="@drawable/kentiaraparlamenti"/>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="260dp"
        android:layout_gravity="bottom">
        <Button
            android:id="@+id/down1"
            android:layout_width="50dp"
            android:layout_height="30sp"
            android:background="@drawable/down"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="20sp"/>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_marginTop="80sp"
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_height="60dp">
            <TextView
                android:id="@+id/videodaiwye3"
                android:textStyle="italic"
                android:textSize="20sp"
                android:layout_weight="25"
                android:text="video"
                android:textAlignment="center"
                android:layout_width="match_parent"
                android:layout_height="40dp"/>
            <TextView
                android:id="@+id/vr3"
                android:layout_weight="25"
                android:textStyle="italic"
                android:textSize="20sp"
                android:text="VR"
                android:textAlignment="center"
                android:layout_width="match_parent"
                android:layout_height="40dp"/>

            <TextView
                android:id="@+id/navigation2"
                android:layout_weight="25"
                android:textStyle="italic"
                android:textSize="20sp"
                android:text="photo"
                android:textAlignment="center"
                android:layout_width="match_parent"
                android:layout_height="40dp"/>

            <TextView
                android:textColor="#000"
                android:layout_weight="25"
                android:textStyle="italic"
                android:textSize="20sp"
                android:text="info"
                android:textAlignment="center"
                android:layout_width="match_parent"
                android:layout_height="40dp"/>


        </LinearLayout>
        <androidx.core.widget.NestedScrollView
            android:layout_gravity="bottom"
            android:layout_width="match_parent"
            android:layout_height="120dp">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:textColor="#000"
                    android:textStyle="normal"
                    android:textSize="25sp"
                    android:gravity="center_vertical"
                    android:text="* Georgian National Museum"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_margin="5dp"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center_vertical"
                        android:background="@drawable/satarr"/>
                    <ImageView
                        android:layout_margin="5dp"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center_vertical"
                        android:background="@drawable/satarr"/>
                    <ImageView
                        android:layout_margin="5dp"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center_vertical"
                        android:background="@drawable/satarr"/>
                    <ImageView
                        android:layout_margin="5dp"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center_vertical"
                        android:background="@drawable/satarr"/>
                    <ImageView
                        android:layout_margin="5dp"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center_vertical"
                        android:background="@drawable/satarr"/>
                </LinearLayout>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:textColor="#000"
                    android:textStyle="normal"
                    android:textSize="25sp"
                    android:gravity="center_vertical"
                    android:text="* Georgian National Museum"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_margin="5dp"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center_vertical"
                        android:background="@drawable/satarr"/>
                    <ImageView
                        android:layout_margin="5dp"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center_vertical"
                        android:background="@drawable/satarr"/>
                    <ImageView
                        android:layout_margin="5dp"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center_vertical"
                        android:background="@drawable/satarr"/>
                    <ImageView
                        android:layout_margin="5dp"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center_vertical"
                        android:background="@drawable/satarr"/>
                    <ImageView
                        android:layout_margin="5dp"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_gravity="center_vertical"
                        android:background="@drawable/satarr"/>
                </LinearLayout>

            </LinearLayout>

        </androidx.core.widget.NestedScrollView>

    </FrameLayout>


</FrameLayout>